{
  "uuid": "e2275913-24cd-4534-a09c-46d55f11a5d6",
  "name": "Collect activity logs from Workday",
  "description": "Fetch activity logs from Workday Activity Logging API",
  "docker_parameters": "workday_activity_logging",
  "arguments": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Workday Activity Logging Configuration",
    "properties": {
      "intake_server": {
        "type": "string",
        "description": "Server of the intake server",
        "default": "https://intake.sekoia.io"
      },
      "intake_key": {
        "type": "string",
        "description": "Intake key for authentication"
      },
      "frequency": {
        "type": "integer",
        "description": "Polling interval in seconds",
        "default": 600,
        "minimum": 60
      },
      "chunk_size": {
        "type": "integer",
        "description": "Maximum chunk size for batch processing",
        "default": 1000,
        "minimum": 1
      },
      "limit": {
        "type": "integer",
        "description": "Number of records to fetch per API request",
        "default": 1000,
        "maximum": 1000
      }
    },
    "required": ["intake_key"]
  }
}